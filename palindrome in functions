#include <stdio.h>
#include <stdbool.h>
bool isPalindrome(int num) {
int original = num;
int reversed = 0;

while (num != 0) {
int digit = num % 10;
reversed = reversed * 10 + digit;
num /= 10;
}

return original == reversed;
}

int main() {
int number;
printf("Enter a number: ");
scanf("%d", &number);
if (isPalindrome(number))
printf("%d is a palindrome.\n", number);
else
printf("%d is not a palindrome.\n", number);

return 0;
}
